package hoja2Bucles;
// Crea un programa que escriba los 50 primeros

// n√∫meros pares.

public class Ejer07 {

	public static void main(String[] args) {

		System.out.println("Los 50 primeros numeros pares:");

		int num = 0;

		while (num <= 100) {

			System.out.println(num);

			num += 2;

		}

	}

}