package ejemplosBucles;

public class Ejemplo01 {
	
	public static void main(String[] args) {
		
		System.out.println("NÃºmeros del 1 al 5:\n");
		
		int contador = 1;
		
		while(contador <= 5) {
			
			System.out.println(contador);
			contador++;
			
		}
		
		System.out.println("Hecho.");
		
	}

}
