package hoja2Bucles;

public class Ejer15b {

	public static void main(String[] args) {

		System.out.println("¡¡¡Potencias de 3!!!");

		int exp = 0;
		double potencia = 1;

		do {
			potencia =  Math.pow(3, exp);
			System.out.println("3 ^ " + exp + " = " + potencia);
			exp++;
		} while (potencia < 12000);
	}

}
