package hoja2Bucles;

import java.util.Scanner;

public class Ejer05 {

	public static void main(String[] args) {

		Scanner entrada = new Scanner(System.in);

		System.out.println("Introduce un n√∫mero: ");

		int num = entrada.nextInt();

		int mult = 1;

		while (mult <= 10) {

			System.out.println(num + " x " + mult + " = " + (num * mult));

			mult++;

		}

	}

}