package hoja2Bucles;

import java.util.Scanner;

public class Ejer09 {

	public static void main(String[] args) {

		Scanner entrada = new Scanner(System.in);

		int dividendo, num2, resultado;

		System.out.println("Escribe dos números enteros");

		System.out.print("Número1: ");
		dividendo = entrada.nextInt();

		System.out.print("Número2: ");
		num2 = entrada.nextInt();

		resultado = dividendo - num2;

		System.out.println(dividendo + " - " + num2 + " = " + resultado);

		dividendo = resultado;

		while (resultado >= num2) {

			resultado = dividendo - num2;
			
			System.out.println(dividendo + " - " + num2 + " = " + resultado);

			dividendo = resultado;

			

		}

	}

}