import java.util.Scanner;


public class Ejer26 {


public static void main(String[] args) {

System.out.println("Introduzca cuando quiere usted saber el horario de la tienda:\nEntre semana o Fin de semana.");

Scanner entrada= new Scanner(System.in);

String dia= entrada.nextLine();

dia=dia.toLowerCase();

String hora;

hora= dia.equals("entre semana")? "20.00":"14.00";

System.out.println("La hora de cierre es a las "+hora+"h.");

entrada.close();

}


}